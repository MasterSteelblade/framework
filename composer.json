{
    "name": "mastersteelblade/framework",
    "type": "library",
    "description": "Simple framework to make my life easier.",

    "homepage": "https://github.com/MasterSteelblade/framework",
    "license": "MIT", 
    "authors": [
        {
            "name": "Benjamin Clarke",
            "email": "mastersteelblade@protonmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ramsey/uuid": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Steelblade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Steelblade\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    }
}
